{
  "name": "pinboard-tag-editor",
  "version": "1.0.0",
  "description": "So, [pinboard](https://pinboard.in) is a thing. It's an awesome thing, and since I started using it I've gone back and hunted down links that otherwise would have been lost forever.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "express": "^4.13.3",
    "fetch": "^0.3.6",
    "fuzzy": "^0.1.1",
    "history": "^1.13.1",
    "immutable": "^3.7.5",
    "jsnox": "^2.1.1",
    "node-fetch": "^1.3.3",
    "ramda": "^0.18.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "electron-prebuilt": "^0.35.1"
  },
  "scripts": {
    "start": "electron build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:be && npm run build:fe",
    "build:watch": "npm run build:be:watch & npm run build:fe:watch",
    "build:be": "babel src --out-dir build",
    "build:be:watch": "babel --watch src --out-dir build",
    "build:fe": "browserify -t babelify assets/js/app.js -o public/js/bundle.js",
    "build:fe:watch": "watchify -t babelify assets/js/app.js -o public/js/bundle.js"
  },
  "author": "Sam Marshall",
  "license": "MIT"
}
